{
  "name": "dcodegroup/laravel-xero-timesheet-sync",
  "description": "This package provides the standard xero functionality for syncing timesheets from your app to Xero.",
  "keywords": [
    "laravel",
    "xero",
    "dcode",
    "timesheets",
    "sync"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Nathan Rzepecki",
      "email": "nathan@lionslair.net.au",
      "homepage": "https://www.lionslair.net.au",
      "role": "Developer"
    },
    {
      "name": "Dcode Group",
      "email": "forge@dcodegroup.com",
      "homepage": "https://dcodegroup.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "dcodegroup/laravel-xero-payroll-au": "@dev|dev-develop|^0.1",
    "dcodegroup/laravel-timesheets": "^0.1",
    "dcodegroup/laravel-xero-oauth": "^1.0",
    "dcodegroup/laravel-xero-employee": "^0.1",
    "laravel/framework": "^7.0 |^8.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "autoload": {
    "psr-4": {
      "Dcodegroup\\LaravelXeroTimesheetSync\\": "src"
    }
  },
  "scripts": {
    "psalm": "vendor/bin/psalm",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dcodegroup\\LaravelXeroTimesheetSync\\LaravelXeroTimesheetSyncServiceProvider"
      ]
    }
  }
}
